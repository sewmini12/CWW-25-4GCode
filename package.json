{
  "name": "skincare-ai",
  "version": "1.0.0",
  "description": "Skin disease detection web application with AI-powered analysis",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend && npm run install:ai",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:ai": "cd ai-service && pip install -r requirements.txt",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:ai\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev:ai": "cd ai-service && python app.py",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "skin-disease",
    "ai",
    "healthcare",
    "medical",
    "diagnosis",
    "react",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2"
  }
}
